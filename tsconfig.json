{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "avatar": [
        "dist/avatar/avatar",
        "dist/avatar"
      ],
      "angular-avatar": [
        "dist/angular-avatar/angular-avatar",
        "dist/angular-avatar"
      ],
      "avatar-angular-kapibara": [
        "dist/avatar-angular-kapibara/avatar-angular-kapibara",
        "dist/avatar-angular-kapibara"
      ]
    }
  }
}